{
  "presets": [
    "@babel/react",
    [
      "@babel/env", {
        "targets": {
          "browsers": "> 1%, not dead"
        },
        "exclude": ["transform-regenerator", "transform-async-to-generator"],
        "debug": true,
        "modules": false,
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": ["async-to-promises"]
}
